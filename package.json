{
    "name": "drawphone",
    "version": "1.0.0",
    "engines": {
        "node": "15.x",
        "npm": "6.x"
    },
    "description": "Telephone with pictures",
    "scripts": {
        "start": "run-s start:dev build:dev",
        "dev": "run-p start:dev build:dev",
        "start:dev": "nodemon --exec npm run start:prod --ignore server/public/ --delay 2",
        "build:dev": "webpack --mode development --watch",
        "start:prod": "node --experimental-json-modules ./server/bin/www",
        "build:prod": "webpack --mode production",
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
        "format": "prettier --write **/*.{js,ts,tsx,json,yml,md}",
        "lint": "eslint . --ext js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tannerkrewson/drawphone.git"
    },
    "author": "Tanner Krewson",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tannerkrewson/drawphone/issues"
    },
    "homepage": "https://github.com/tannerkrewson/drawphone#readme",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.2",
        "assets-webpack-plugin": "^7.0.0",
        "aws-sdk": "^2.848.0",
        "blueimp-canvas-to-blob": "^3.28.0",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.6.0",
        "bootstrap-sweetalert": "^1.0.1",
        "canvas": "^2.7.0",
        "cookie-parser": "^1.4.5",
        "css-loader": "^5.0.2",
        "debug": "^4.3.1",
        "dexie": "^3.0.3",
        "express": "^4.17.1",
        "fabric": "^4.3.1",
        "file-loader": "^6.2.0",
        "got": "^11.8.1",
        "jquery": "^3.5.1",
        "knuth-shuffle": "^1.0.8",
        "mini-css-extract-plugin": "^1.3.8",
        "ml5": "^0.6.0",
        "morgan": "^1.10.0",
        "npm-run-all": "^4.1.5",
        "popper.js": "^1.16.1",
        "pug": "^3.0.0",
        "socket.io": "^3.1.1",
        "socket.io-client": "^3.1.1",
        "striptags": "^3.1.1",
        "typeface-pangolin": "1.1.13",
        "url-loader": "^4.1.1",
        "webpack": "^5.23.0",
        "webpack-cli": "^4.5.0"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^7.2.0",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.4",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1"
    },
    "lint-staged": {
        "*.@(js|ts|tsx|json|md)": [
            "npm run lint",
            "npm run format"
        ]
    },
    "type": "module"
}
